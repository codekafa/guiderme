                                <a href="javascript:void(0);" data-toggle="modal" data-target="#user-register">@lexiconHelper.GetTextValue("_become_a_user", 1)</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link header-login" href="javascript:void(0);" data-toggle="modal" data-target="#login_modal">@lexiconHelper.GetTextValue("_login", 1)</a>
                            </li>
                        }
                        else
                        {

                            var currentUser = securtyService.GetCurrentUser();

                            string urlPhoto = "";
                            string fileTransform = currentUser.GetFileNameFromUserPhoto();
                            urlPhoto = "https://res.cloudinary.com/servicebuilder/image/upload/w_300,h_300/" + fileTransform;
                            var notifyResult = notifyService.GetUnReadNotifications(currentUser.ID);
                            var notifyList = notifyResult.Data as List<ViewModel.Views.Notification.NotificationListModel>;

                            <li class="nav-item dropdown">
                                <a href="#" class="" data-toggle="dropdown">
                                    <i class="fas fa-bell"></i> <span class="badge badge-pill bg-yellow">@notifyResult.DataCount</span>
                                </a>
